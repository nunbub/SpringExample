<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.nunbub.spring.ex.database.dao.UsedGoodsDAO">

	<select id="selectUsedGoodsList" resultType="com.nunbub.spring.ex.database.model.UsedGoods">
		SELECT 
			`id`
			, `sellerID`
			, `title`
			, `price`
			, `description`
			, `picture`
			, `createdAt`
			, `updatedAt`				
		FROM 
			`used_goods`;
		
		
	</select>
	
</mapper>